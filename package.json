{
  "name": "skedge",
  "version": "1.0.0",
  "engines": {
    "node": "8.11.2"
  },
  "description": "",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "server": "nodemon server.js",
    "test": "npx jest --silent",
    "test:update": "jest --silent -u",
    "test:watch": "jest --silent --watch",
    "test:coverage": "jest --silent --coverage",
    "docs": "docsify serve docs",
    "client": "npm run start --prefix client",
    "dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/skedge-io/skedge.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/skedge-io/skedge/issues"
  },
  "homepage": "https://github.com/skedge-io/skedge#readme",
  "dependencies": {
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.18.3",
    "concurrently": "^3.6.1",
    "cookie-session": "^2.0.0-beta.3",
    "express": "^4.16.4",
    "google-auth-library": "^1.6.1",
    "google-calendar": "^1.3.2",
    "googleapis": "^32.0.0",
    "localtunnel": "^1.9.1",
    "moment": "^2.22.2",
    "mongoose": "^5.3.5",
    "node-schedule": "^1.3.0",
    "passport": "^0.4.0",
    "passport-google-oauth20": "^1.0.0",
    "passport-oauth2-refresh": "^1.1.0",
    "twilio": "^3.23.1"
  },
  "devDependencies": {
    "jest": "^23.6.0",
    "react-test-renderer": "^16.6.0"
  }
}
